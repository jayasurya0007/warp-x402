{
  "name": "warp402-sdk",
  "version": "1.0.0",
  "description": "Cross-chain payment receipt SDK for Avalanche Subnets using Teleporter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "ts-node test/sdk-test.ts",
    "test:unit": "ts-node test/sdk-test.ts",
    "test:errors": "ts-node test/error-handling.ts",
    "test:local": "ts-node test/integration-local.ts",
    "test:fuji": "ts-node test/integration-fuji.ts",
    "test:all": "npm run test:unit && npm run test:errors",
    "example:local": "ts-node examples/local-demo.ts",
    "example:fuji": "ts-node examples/fuji-demo.ts",
    "example:cross": "ts-node examples/cross-subnet-demo.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "avalanche",
    "subnet",
    "teleporter",
    "cross-chain",
    "payment",
    "http402",
    "warp"
  ],
  "author": "Warp-402 Team",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
